[ignore]

; Ignore unexpected extra "@providesModule"
.*/node_modules/.*/node_modules/fbjs/.*
.*/node_modules/.*/node_modules/warning/.*

; Ignore polyfills
.*/Libraries/polyfills/.*

; Ignore rc-util
.*/node_modules/rc-util/.*

; Broken JSON
.*/node_modules/config-chain/test/.*

[include]

[libs]

[untyped]
.*/node_modules/styled-components-breakpoint
.*/node_modules/redux-persist
.*/node_modules/jss
.*/node_modules/react-jss
.*/node_modules/redux-saga-test-plan
.*/node_modules/react-select
.*/node_modules/polished

[options]
emoji=true

module.system=haste

munge_underscores=true

module.name_mapper='^@components\(.*\)$' -> '<PROJECT_ROOT>/src/components\1'
module.name_mapper='^@config\(.*\)$' -> '<PROJECT_ROOT>/src/config\1'
module.name_mapper='^@graphql\(.*\)$' -> '<PROJECT_ROOT>/src/graphql\1'
module.name_mapper='^@modules\(.*\)$' -> '<PROJECT_ROOT>/src/modules\1'
module.name_mapper='^@services\(.*\)$' -> '<PROJECT_ROOT>/src/services\1'
module.name_mapper='^@store\(.*\)$' -> '<PROJECT_ROOT>/src/store\1'
module.name_mapper='^@theme\(.*\)$' -> '<PROJECT_ROOT>/src/theme\1'

module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FlowFixMeProps
suppress_type=$FlowFixMeState

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe\\($\\|[^(]\\|(\\(<VERSION>\\)? *\\(site=[a-z,_]*react_native[a-z,_]*\\)?)\\)
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue\\((\\(<VERSION>\\)? *\\(site=[a-z,_]*react_native[a-z,_]*\\)?)\\)?:? #[0-9]+
suppress_comment=\\(.\\|\n\\)*\\$FlowFixedInNextDeploy
suppress_comment=\\(.\\|\n\\)*\\$FlowExpectedError

server.max_workers=4

[version]
^0.95.1
